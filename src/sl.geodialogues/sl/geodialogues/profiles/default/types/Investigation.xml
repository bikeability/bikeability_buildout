<?xml version="1.0"?>
<object name="Investigation"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="plone" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Measurement</property>
 <property name="description"
    i18n:translate="">An Investiagtion</property>
 <property name="icon_expr">string:${portal_url}/investigation_icon.png</property>
 <property name="content_meta_type">Investiagtion</property>
 <property name="product">sl.geodialogues</property>
 <property name="factory">addInvestiagtion</property>
 <property name="add_view_expr"></property>
 <property name="link_target"></property>
 <property name="immediate_view">atct_edit</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
 	 <element value="Measurement"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">measurement_view</property>
 <property name="view_methods">
  <element value="view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="base_edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}/investigation_view"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}/edit"
    visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
